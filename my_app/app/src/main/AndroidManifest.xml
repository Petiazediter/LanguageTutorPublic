<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codecool.languagetutor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/short_app_name"
        android:roundIcon="@mipmap/granny_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".quiz_layout.QuizActivity" />
        <activity android:name=".newword.NewWordActivity" />
        <activity android:name=".myword.MyWordActivity" />
        <activity android:name=".ResultHistoryActivity" />
        <activity android:name=".main_activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>